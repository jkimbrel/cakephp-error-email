{
    "name": "jkimbrel/cakephp-error-email",
    "description": "CakePHP3.x ErrorEmail Plugin",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "error", "exception", "email", "plugin"],
    "homepage": "https://github.com/jkimbrel/cakephp-error-email/",
    "license": "MIT",
    "authors": [
        {
            "name": "Ethan Brigham",
            "email": "ebrigham1@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/jkimbrel/cakephp-error-email/issues",
        "source": "https://github.com/jkimbrel/cakephp-error-email/"
    },
    "require": {
        "php": ">=5.6",
        "cakephp/cakephp": "^3.3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "cakephp/cakephp-codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "ErrorEmail\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ErrorEmail\\Test\\": "tests"
        }
    }
}
